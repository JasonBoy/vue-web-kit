{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["> 0.5%", "not ie 11", "not op_mini all"]
        },
        "loose": true,
        "useBuiltIns": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "lodash",
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-vue-jsx",
    "transform-class-properties",
    "syntax-dynamic-import"
  ],
  "comments": false
}