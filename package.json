{
  "name": "koa-web-kit",
  "version": "2.1.4",
  "description": "A modern, production-ready, and full-stack node web framework",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "deploy": "./deploy.sh",
    "build": "webpack --progress --hide-modules --config config/webpack.config.dev.js && echo Build Done!",
    "watch": "webpack --watch --hide-modules --progress --config config/webpack.config.dev.js",
    "node": "webpack --progress --config config/webpack.config.node.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "lint": "eslint --ext .js --cache .",
    "es6-module-to-commonjs": "babel test/es6/modules -d build/test/es6/modules --plugins transform-es2015-modules-commonjs",
    "test": "./node_modules/.bin/_mocha ./test/mocha"
  },
  "engines": {
    "node": ">= 8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/koa-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "vue",
    "bootstrap",
    "fullstack",
    "framework"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/koa-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/koa-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bluebird": "3.5.1",
    "bootstrap": "4.0.0-beta.3",
    "consolidate": "0.15.0",
    "koa": "2.4.1",
    "koa-better-body": "3.0.4",
    "koa-compress": "2.0.0",
    "koa-convert": "1.2.0",
    "koa-morgan": "1.0.1",
    "koa-mount": "3.0.0",
    "koa-router": "7.3.0",
    "koa-session": "5.8.1",
    "koa-static": "4.0.2",
    "koa-views": "6.1.3",
    "lodash": "4.17.4",
    "lodash.isempty": "4.4.0",
    "moment": "2.20.1",
    "nunjucks": "3.0.1",
    "qs": "6.5.1",
    "request": "2.83.0",
    "request-debug": "0.2.0",
    "url-parse": "1.2.0",
    "vue": "2.5.13",
    "vue-router": "3.0.1",
    "whatwg-fetch": "2.0.3",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "0.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.17",
    "copy-webpack-plugin": "4.3.1",
    "cross-env": "5.1.3",
    "css-loader": "0.28.9",
    "del": "3.0.0",
    "eslint": "4.15.0",
    "eslint-loader": "1.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "mocha": "4.0.1",
    "node-sass": "4.7.2",
    "postcss-loader": "2.0.10",
