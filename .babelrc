{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["> 1%", "last 5 versions"]
        },
        "loose": true,
        "modules": false
      }
    ]
  ],
  "plugins": [
    "transform-runtime",
    "transform-es2015-modules-commonjs",
    "transform-object-rest-spread"
  ],
  "comments": false
}