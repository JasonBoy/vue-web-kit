{
  "name": "vue-web-kit",
  "version": "1.3.1",
  "description": "A modern, production-ready, and full-stack node web framework with Vue.js",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env ENABLE_HMR=true node app.js",
    "start": "nodemon app.js",
    "deploy": "./deploy.sh",
    "deploy:noinstall": "npm run deploy -- 1",
    "build:dev": "webpack --progress  --hide-modules --config config/webpack.config.dev.js",
    "build": "npm run prod",
    "watch": "webpack --watch --hide-modules --progress --config config/webpack.config.dev.js",
    "node": "webpack --progress --config config/webpack.config.node.js",
    "prod": "cross-env NODE_ENV=production webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "prod:analyze": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack -p --progress --hide-modules --config config/webpack.config.prod.js",
    "test": "npm run build:dev && npm run build",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">= 8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,jsx,vue,scss,json,html}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JasonBoy/vue-web-kit.git"
  },
  "keywords": [
    "koa",
    "es6",
    "webpack",
    "vue",
    "bootstrap",
    "fullstack",
    "framework"
  ],
  "author": "jasonlikenfs@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JasonBoy/vue-web-kit/issues"
  },
  "homepage": "https://github.com/JasonBoy/vue-web-kit#readme",
  "readme": "README.md",
  "dependencies": {
    "bootstrap": "4.2.1",
    "chalk": "2.4.1",
    "consolidate": "0.15.1",
    "koa": "2.6.2",
    "koa-body": "4.0.6",
    "koa-compress": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-helmet": "4.0.0",
    "koa-history-api-fallback": "0.1.3",
    "koa-morgan": "1.0.1",
    "koa-mount": "4.0.0",
    "koa-router": "7.4.0",
    "koa-session": "5.10.1",
    "koa-static": "5.0.0",
    "koa-views": "6.1.5",
    "lodash": "4.17.11",
    "lodash.isempty": "4.4.0",
    "moment": "2.23.0",
    "nunjucks": "3.1.6",
    "pm2": "3.2.4",
    "qs": "6.6.0",
    "request": "2.88.0",
    "request-debug": "0.2.0",
    "url-parse": "1.4.4",
    "vue": "2.5.21",
    "vue-router": "3.0.2",
    "whatwg-fetch": "2.0.4",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "autoprefixer": "9.4.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-loader": "7.1.5",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-vue-jsx": "3.7.0",
    "babel-preset-env": "1.7.0",
    "clean-webpack-plugin": "1.0.0",
    "copy-webpack-plugin": "4.6.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-inline-chunk-plugin": "1.1.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "koa-webpack": "5.1.1",
    "lint-staged": "8.1.0",
    "mini-css-extract-plugin": "0.4.1",
    "moment-locales-webpack-plugin": "1.0.7",
    "node-sass": "4.11.0",
    "postcss-loader": "2.1.6",
    "prettier": "1.15.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "1.1.2",
    "vue-loader": "15.5.0",
    "vue-template-compiler": "2.5.21",
    "webpack": "4.16.3",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "browserslist": [
    "> 0.5%",
    "not ie 11",
    "not op_mini all"
  ],
  "nodemonConfig": {
    "watch": [
      "api/",
      "config/",
      "build/node",
      "routes/",
      "utils/",
      "services/"
    ]
  }
}
